{
  "name": "smart-transport-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Smart School Transport & Attendance System - Frontend",
  "author": "King'ori Joseph Ndiritu",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,scss,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,scss,md,json}\"",
    "analyze": "source-map-explorer 'dist/assets/*.js'",
    "clean": "rm -rf dist node_modules .cache",
    "build:analyze": "vite build --analyze",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-google-maps/api": "^2.20.8",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.6.3",
    "jspdf": "^4.2.0",
    "jspdf-autotable": "^5.0.7",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet-routing-machine": "^3.2.12",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-datepicker": "^7.6.0",
    "react-dom": "^19.2.0",
    "react-grid-layout": "^2.2.2",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "^5.0.0",
    "react-resizable": "^3.1.3",
    "react-router-dom": "^7.13.0",
    "react-select": "^5.10.1",
    "react-transition-group": "^4.4.5",
    "recharts": "^3.7.0",
    "socket.io-client": "^4.8.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/file-saver": "^2.0.7",
    "@types/leaflet": "^1.9.17",
    "@types/leaflet-routing-machine": "^3.2.5",
    "@types/lodash": "^4.17.16",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.2.7",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^19.2.3",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^4.1.4",
    "vite": "^8.0.0-beta.13"
  },
  "overrides": {
    "vite": "^8.0.0-beta.13"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "school-transport",
    "attendance-system",
    "gps-tracking",
    "real-time",
    "react",
    "leaflet-maps"
  ]
}
